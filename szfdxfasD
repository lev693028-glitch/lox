        super().__init__("Кіт", "Мяу-мяу")
        self.кличка = кличка
        class ТранспортнийЗасіб:
    def __init__(self, швидкість):
        self.швидкість = швидкість

    def переміщення(self):
        print(f"Транспорт рухається зі швидкістю {self.швидкість} км/год.")

class Автомобіль(ТранспортнийЗасіб):
    def переміщення(self):
        print(f"Автомобіль їде зі швидкістю {self.швидкість} км/год.")

class Літак(ТранспортнийЗасіб):
    def переміщення(self):
        print(f"Літак летить зі швидкістю {self.швидкість} км/год.")
