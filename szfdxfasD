class Тварина:
    def __init__(self, вид, звук):
        self.вид = вид
        self.звук = звук

    def говорити(self):
        print(f"{self.вид} видає звук: {self.звук}")

class Собака(Тварина):
    def __init__(self, кличка):
        super().__init__("Собака", "Гав-гав")
        self.кличка = кличка

class Кіт(Тварина):
    def __init__(self, кличка):
        super().__init__("Кіт", "Мяу-мяу")
        self.кличка = кличка

